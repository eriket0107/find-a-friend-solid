{
  "name": "find-a-friend-solid",
  "version": "1.0.0",
  "description": "## To start application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "tsx watch dist/app.cjs",
    "build": "tsup src --out-dir dist",
    "dev": "tsx watch server.ts",
    "test:use-cases": "vitest run --dir src/use-cases",
    "test:watch-use-cases": "vitest --dir src/use-cases",
    "test": "vitest run --dir src",
    "test:watch": "vitest --dir src",
    "test:coverage": "vitest --coverage",
    "typeorm": "tsx node_modules/typeorm/cli.js -d database/data-source",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run",
    "seed:organizations": "tsx scripts/seed-organization.ts",
    "lint": "eslint --fix --ignore-pattern 'dist/*'",
    "lint:ci": "yarn eslint --fix --ignore-pattern 'dist/*'",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.19.0",
    "@faker-js/faker": "9.4.0",
    "@types/amqplib": "0.10.6",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.13.0",
    "@types/swagger-ui-dist": "3.30.5",
    "@vitest/coverage-v8": "3.0.5",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "15.14.0",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "tsup": "8.3.6",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.22.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "10.0.2",
    "@fastify/jwt": "9.0.3",
    "@fastify/multipart": "9.0.3",
    "@fastify/static": "8.1.1",
    "@fastify/swagger": "9.4.2",
    "@fastify/swagger-ui": "5.2.1",
    "amqplib": "0.10.5",
    "axios": "1.8.4",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "husky": "9.1.7",
    "pg": "8.13.1",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "reflect-metadata": "0.2.2",
    "sharp": "0.33.5",
    "swagger-ui-dist": "5.20.1",
    "typeorm": "0.3.20",
    "zod": "3.24.1"
  }
}
